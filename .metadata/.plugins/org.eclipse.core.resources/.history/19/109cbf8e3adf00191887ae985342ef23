import java.io.*;
 class NoSync extends Thread{
	 NoSync a = new NoSync();
	 public void Run() {
		 a.Display();
	 }
	 public static void main(String[] args) {
			Sync a = new Sync();
			Sync b = new Sync();
		}
}
 public class Sync extends Thread {
	 public static synchronized void Display() {
		 for(int i =0;i<10;i++) {
			 System.out.println(i);
			 try {
				 Thread.sleep(1000);
			 }
			 catch(Exception e) {}
		 }
	 }
		
	}
